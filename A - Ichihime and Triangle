#include<iostream>
#include<algorithm>
#include<vector>
#include<cmath>
#define lli long long int
using namespace std;
int main()
{
	ios_base::sync_with_stdio(false);
	cin.tie(NULL);
	lli t;
	cin >> t;
	while (t--)
	{
		lli a, b, c, d;
		cin >> a >> b >> c >> d;
		cout << b << " " << c << " " << c << '\n';
	}
	return 0;
}
