#include<iostream>
#include<algorithm>
#define lli long long int
using namespace std;
int main()
{
	int t;
	cin >> t;
	while (t--)
	{
		int n, m,flag=0;
		cin >> n >> m;
		if (n == 1 || m == 1)
			flag = 1;
		else if (n == 2 && m == 2)
			flag = 1;
		if (flag == 1)
			cout << "YES" << '\n';
		else cout << "NO" << '\n';
	}
	return 0;
}
